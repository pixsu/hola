import java.util.*;

public class Main {
 
  public static void main(String[] args) {

    Scanner sc = new Scanner(System.in);

    String name;
    String section;
    double midterms=90;
    double finals=97;

    System.out.println("Enter Name: ");
    name = sc.nextLine();
    System.out.println("Enter Section: ");
    section = sc.nextLine();

    System.out.println("Midterm grade: "+ midterms);
    System.out.println("Finals grade: "+ finals);

    double average= (midterms+finals)/2;
    System.out.println("Hello "+ name + ", your GWA is: "+ average);


  }
}
